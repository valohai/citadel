[run]
branch = True
omit =
    *management/commands*
    *migrations*
    *site-packages*
    *venv*
    *virtualenv*
    manage.py
    citadel/settings.py
    citadel/wsgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
